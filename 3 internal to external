#include <pic18f4550.h> 
#pragma config FOSC = HS 
#pragma config WDT = OFF 
#pragma config LVP = OFF 
#pragma config PBADEN = OFF 
void main(void) { 
 int i, temp; 
 int source1[] = {0x21, 0x22, 0x23, 0x24, 0x25}; 
 int dest[] = {0x99, 0x99, 0x99, 0x99, 0x99}; 
 for (i = 0; i < 5; i++) { 
 temp = source1[i]; 
 source1[i] = dest[i]; 
 dest[i] = temp; 
 } 
} 
